{
    "name": "generator-ajslib",
    "version": "1.4.0",
    "description": "Yeoman Gulp Generator for AngularJS Library",
    "homepage": "https://github.com/yadickson/generator-ajslib",
    "author": {
        "name": "Yadickson Soto",
        "email": "yadickson@gmail.com",
        "url": "https://github.com/yadickson"
    },
    "files": [
        "generators/app",
        "generators/component",
        "generators/constant",
        "generators/decorator",
        "generators/directive",
        "generators/factory",
        "generators/filter",
        "generators/provider",
