        "chalk": "^2.3.0",
        "command-exists": "^1.2.2",
        "decamelize": "^1.2.0",
        "generator-ajsbase": "^1.3.2",
        "package-json-utils": "^1.1.0",
        "upper-case": "^1.1.3",
        "uppercamelcase": "^3.0.0",
        "yeoman-generator": "^2.0.1",
        "yosay": "^2.0.0"
    },
    "devDependencies": {
        "coveralls": "^3.0.0",
        "mocha": "^4.0.1",
        "nyc": "^11.3.0",
        "rimraf": "^2.6.2",
        "yeoman-assert": "^3.0.0",
        "yeoman-test": "^1.6.0"
    },
    "scripts": {
        "clean": "./node_modules/.bin/rimraf coverage reports *.tgz",
        "test": "./node_modules/.bin/nyc --reporter=html mocha --recursive",
